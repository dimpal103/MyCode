package arraysQ;

public class BinarySearch {
	public static int doBinarySerch (int[]a, int first, int last,int key) {
		
		int mid = first+last-1/2;
		
		if(a[mid] == key) {
			return a[mid];
		}
		
		else if(a[mid] > key) {
			doBinarySerch(a,first,mid-1,key);
		}
		
		else if(a[mid] < key) {
			doBinarySerch(a, mid+1,last,key);
		}
		
		else 
			return 0;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] array = {9,3,1,5};
		int last = array.length;
		int first = 0;
		int mid = 0;
		int key = 1;

	}

}
