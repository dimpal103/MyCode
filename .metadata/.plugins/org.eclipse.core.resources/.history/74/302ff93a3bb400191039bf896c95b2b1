package string;

public class ValueOfComparison {
//Q : What will be value of comparison of “100” and “21” ?
	//Q : Define this function.
	// Returns true if firstString contains secondString ignoring case.
	// Returns false otherwise.

	public static boolean checkStringContains(String s1, String s2) {
		return s1.contains(s2);
	}
	
	public class abc extends ValueOfComparison {
		public boolean checkStringContains(String s1, String s2, String s3) {
			return s1==s2;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String s1 = "100";
		String s2 = "21";
		
		System.out.println(s1.compareTo(s2));
		
		System.out.println(s1.contains(s2));
		
		System.out.println(s1.contains("0"));
		
		String s11 = "anamika";
		String s22 = "num";
		
		System.out.println(checkStringContains(s11, s22));
	}

}
